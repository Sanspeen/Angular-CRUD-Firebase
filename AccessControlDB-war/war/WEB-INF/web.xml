<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<!-- Servlets -->
	<servlet>
		<servlet-name>SqlEngineService</servlet-name>
		<servlet-class>com.pratech.accesscontroldb.server.SqlEngineServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>SqlEngineService</servlet-name>
		<url-pattern>/accesscontroldb_war/sqlengineservice</url-pattern>
	</servlet-mapping>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>AccessControlDB_war.html</welcome-file>
	</welcome-file-list>

</web-app>
